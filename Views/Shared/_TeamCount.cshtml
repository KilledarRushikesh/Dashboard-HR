@model IEnumerable<ThinkBridge.Models.Reports>

@{
    ViewBag.Title = "TeamCount";
}

<h2>TeamCount</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberCount)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemberCount)
            </td>
        </tr>
    }

</table>
